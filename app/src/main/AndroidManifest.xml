<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.laily.fp_mp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FP_MP">
        <activity android:name=".view.MainActivity"></activity>
        <activity android:name=".view.ChooseActivity" />
        <activity android:name=".view.DetailJobActivity" />
        <activity android:name=".view.SignUpActivity" />
        <activity android:name=".view.SignInActivity" />
        <activity android:name=".CompanyActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>